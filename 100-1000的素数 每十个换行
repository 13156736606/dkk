#include "stdio.h"
  int main() 
  {
      int i,j,isprime=1,num=0;
      for(i=100;i<=1000;i++)
      {
          isprime=1;
          for (j=2; j<=i/2; j++)
          {
              if(i%j==0)
              {
                  isprime=0;
                  break;
              }
          }
      }
          if(isprime)
          {
              printf("%4d",i);
              num++;
          }
          if(num%10==0)
          {
              printf("\n");
          }
          
      

  }
